   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated efsm -->
      <editor
            id="Efsm.efsm.presentation.EfsmEditorID"
            name="%_UI_EfsmEditor_label"
            icon="icons/full/obj16/EfsmModelFile.gif"
            extensions="efsm"
            class="Efsm.efsm.presentation.EfsmEditor"
            contributorClass="Efsm.efsm.presentation.EfsmActionBarContributor">
