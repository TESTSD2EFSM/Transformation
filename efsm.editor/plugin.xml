<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated efsm -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="Efsm.efsm.presentation.EfsmModelWizardID"
            name="%_UI_EfsmModelWizard_label"
            class="Efsm.efsm.presentation.EfsmModelWizard"
